{
  "name": "antd-mobile-vue",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cd example && parcel  ./index.html",
    "build": "npx rollup -c",
    "api": "api-extractor run",
    "lint": "prettier --write  \"packages/**/*.ts?(x)\" \"example/**/*.ts?(x)\""
  },
  "dependencies": {
    "css-animation": "^2.0.4",
    "normalize.css": "^8.0.1",
    "vue": "3.0.0-beta.10"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@microsoft/api-extractor": "^7.8.0",
    "@types/react": "^16.9.34",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@rollup/plugin-commonjs": "^11.0.1",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "autoprefixer": "^9.7.3",
    "less": "^3.11.1",
    "lint-staged": "^9.5.0",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.0.5",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-typescript2": "^0.27.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.8.3"
  },
    "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write",
      "git add"
    ]
  },
  "alias": {
    "packages": "./packages"
  }
}